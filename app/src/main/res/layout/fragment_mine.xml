                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dimen_16px"
                    android:drawableLeft="@mipmap/mind_xxtx_icon"
                    android:drawablePadding="@dimen/text_10px"
                    android:text="我的健康秘钥"
                    android:textColor="@color/text_color"
                    android:textSize="@dimen/text_14px" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/dimen_16px"
                    android:background="@mipmap/my_arrow" />
            </RelativeLayout>

            <View
